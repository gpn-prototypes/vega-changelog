{
  "name": "@gpn-prototypes/vega-changelog",
  "version": "3.2.4",
  "keywords": [
    "changelog",
    "lerna"
  ],
  "author": "gpn-prototypes",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "changelog": "./bin/cli.js"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:gpn-prototypes/vega-changelog.git"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "changelog": "node ./bin/cli.js",
    "fix": "npm run lint --fix",
    "lint": "eslint src --ext ts --format stylish",
    "release": "standard-version",
    "prepublish": "npm run build",
    "prettier": "prettier --write 'src/**/*.ts'",
    "test": "jest",
    "test-ci": "npm run build && jest",
    "watch": "npm run build --watch"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "cli-highlight": "^2.1.4",
    "execa": "^1.0.0",
    "hosted-git-info": "^3.0.4",
    "make-fetch-happen": "^7.1.1",
    "progress": "^2.0.0",
    "yargs": "^13.0.0"
  },
  "devDependencies": {
    "@actions/core": "^1.2.4",
    "@types/jest": "^25.2.2",
    "@types/node": "^13.13.4",
    "standard-version": "^8.0.0",
    "@gpn-prototypes/frontend-configs": "^0.1.1",
    "@testing-library/jest-dom": "^5.5.0",
    "fs-extra": "^8.1.0",
    "jest": "^25.5.4",
    "prettier": "1.19.1",
    "ts-jest": "^25.5.1",
    "lint-staged": "^10.2.11",
    "husky": "^4.2.5",
    "commitizen": "^4.1.2",
    "cz-customizable": "^6.2.0",
    "remark": "^12.0.0",
    "typescript": "^3.9.2"
  }
}
